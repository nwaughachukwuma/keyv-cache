{
  "name": "keyv-cache",
  "version": "3.0.1",
  "description": "~1kb key/value store wrapper for the browser cache-api, with zero-dependency",
  "repository": "git@github.com:nwaughachukwuma/keyv-cache.git",
  "author": "Chukwuma Nwaugha <nwaughac@gmail.com>",
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": "./lib/index.js",
  "sideEffects": false,
  "keywords": [
    "key/value cache",
    "browser cache",
    "caching",
    "optimize",
    "performance",
    "ttl",
    "automatic invalidation"
  ],
  "files": [
    "lib",
    "!lib/index.test-d.ts"
  ],
  "scripts": {
    "build": "tsc",
    "compile": "tsc -p .",
    "tsd:prep": "cp index.test-d.ts ./lib/index.test-d.ts",
    "tsd": "yarn tsd:prep && tsd",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.11.12",
    "babel-jest": "^29.5.0",
    "browser-cache-mock": "^0.1.7",
    "delay": "^5.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jsdom": "^22.0.0",
    "ts-node": "^10.9.1",
    "tsd": "^0.28.1",
    "typescript": "^5.0.4",
    "whatwg-fetch": "^3.6.2"
  },
  "engines": {
    "node": ">=16"
  }
}
