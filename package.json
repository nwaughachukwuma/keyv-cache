{
  "name": "keyv-cache",
  "version": "0.0.1",
  "description": "simple key/value cache using the browser cache-api",
  "repository": "git@github.com:nwaughachukwuma/keyv-cache.git",
  "author": "Chukwuma Nwaugha <nwaughac@gmail.com>",
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": "./dist/index.js",
  "sideEffects": false,
