    "tsd:clean": "rm ./dist/index.test-d.ts",
    "test": "jest"
  },
  "devDependencies": {
    "browser-env": "^3.3.0",
    "delay": "^5.0.0",
    "jest": "^29.3.1",
    "jsdom": "^20.0.3",
    "tsd": "^0.17.0",
    "typescript": "^4.9.4"
  }
}
