{
  "name": "keyv-cache",
  "version": "0.0.3",
  "description": "~1kb key/value store wrapper for the browser cache-api, with zero-dependency",
  "repository": "git@github.com:nwaughachukwuma/keyv-cache.git",
  "author": "Chukwuma Nwaugha <nwaughac@gmail.com>",
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": "./lib/index.js",
  "sideEffects": false,
  "keywords": [
    "key/value cache",
    "browser cache",
    "caching",
    "optimize",
    "performance",
    "ttl",
    "automatic invalidation"
  ],
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "compile": "tsc -p .",
    "tsd:prep": "cp index.test-d.ts ./lib/index.test-d.ts",
    "tsd:clean": "rm ./lib/index.test-d.ts",
    "tsd": "yarn tsd:prep && tsd && yarn tsd:clean",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.4",
    "babel-jest": "^29.3.1",
    "browser-cache-mock": "^0.1.7",
    "delay": "^5.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jsdom": "^20.0.3",
    "ts-node": "^10.9.1",
    "tsd": "^0.17.0",
    "typescript": "^4.9.4",
    "whatwg-fetch": "^3.6.2"
  },
  "dependencies": {
    "@types/node": "^18.11.12"
  }
}
