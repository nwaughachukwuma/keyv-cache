{
  "name": "kv-cache",
  "version": "0.0.1",
  "description": "simple key/value cache using the browser cache-api",
  "main": "./dist/index.js",
  "repository": "git@github.com:nwaughachukwuma/kv-cache.git",
  "author": "Chukwuma Nwaugha <nwaughac@gmail.com>",
  "license": "MIT",
  "private": false,
  "type": "module",
	"types": "./dist/index.d.ts",
	"exports": "./dist/index.js",
	"sideEffects": false,
	"keywords": [
		"key/value cache",
		"browser cache",
		"caching",
		"optimize",
		"performance",
		"ttl",
		"automatic invalidation"
	],
  "files": [
		"dist"
	],
	"scripts": {
		"tsc": "tsc",
		"tsd": "yarn tsd:prep && tsd && yarn tsd:clean",
		"tsd:prep": "cp index.test-d.ts ./dist/index.test-d.ts",
		"tsd:clean": "rm ./dist/index.test-d.ts",
		"test": "tsc && ava && yarn tsd"
	},
  "devDependencies": {
    "ava": "^3.15.0",
		"delay": "^5.0.0",
		"tsd": "^0.17.0",
    "typescript": "^4.9.4"
  }
}
